   "outputs": [],
   "source": [
    "@lru_cache(maxsize = None)\n",
    "def fib4(n: int) -> int :\n",
    "    if n < 2:\n",
    "        return n\n",
    "    return fib4(n - 2) + fib4(n - 1)"
   ]
  },
